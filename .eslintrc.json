{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "import/extensions": ["error", "always", { "ignorePackages": false }],
      "import/no-import-module-exports": 0
    }
}
