{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "import/extensions": ["off", "always", { "ignorePackages": false }],
      "import/no-import-module-exports": 0,
      "no-tabs": ["off"],
      "import/no-unresolved": 0,
      "import/order": 0
    },
    "settings": {
      "import/extensions": [".js",".ts",".jsx",".tsx"]
    }
}
